{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"host": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "less"
				}
			},
			"root": "projects/host",
			"sourceRoot": "projects/host/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-architects/native-federation:build",
					"options": {},
					"configurations": {
						"production": {
							"target": "host:esbuild:production"
						},
						"development": {
							"target": "host:esbuild:development",
							"dev": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-architects/native-federation:build",
					"options": {
						"target": "host:serve-original:development",
						"rebuildDelay": 0,
						"dev": true,
						"port": 0
					}
				},
				"esbuild": {
					"builder": "@angular/build:application",
					"options": {
						"outputPath": "dist/host",
						"index": "projects/host/src/index.html",
						"polyfills": [
							"zone.js",
							"es-module-shims"
						],
						"tsConfig": "projects/host/tsconfig.app.json",
						"inlineStyleLanguage": "less",
						"assets": [
							{
								"glob": "**/*",
								"input": "public"
							}
						],
						"styles": [
							"projects/host/src/styles.less"
						],
						"scripts": [],
						"customWebpackConfig": {
							"path": "projects/host/webpack.config.js"
						},
						"browser": "projects/host/src/main.ts"
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "1MB",
									"maximumError": "2MB"
								}
							],
							"outputHashing": "all",
							"customWebpackConfig": {
								"path": "projects/host/webpack.prod.config.js"
							}
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve-original": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"port": 4200
					},
					"configurations": {
						"production": {
							"buildTarget": "host:esbuild:production"
						},
						"development": {
							"buildTarget": "host:esbuild:development"
						}
					},
					"defaultConfiguration": "development"
				}
			}
		},
		"angular-child": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "less"
				}
			},
			"root": "projects/angular-child",
			"sourceRoot": "projects/angular-child/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-architects/native-federation:build",
					"options": {},
					"configurations": {
						"production": {
							"target": "angular-child:esbuild:production"
						},
						"development": {
							"target": "angular-child:esbuild:development",
							"dev": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-architects/native-federation:build",
					"options": {
						"target": "angular-child:serve-original:development",
						"rebuildDelay": 0,
						"dev": true,
						"port": 0
					}
				},
				"esbuild": {
					"builder": "@angular/build:application",
					"options": {
						"outputPath": "dist/angular-child",
						"index": "projects/angular-child/src/index.html",
						"polyfills": [
							"zone.js",
							"es-module-shims"
						],
						"tsConfig": "projects/angular-child/tsconfig.app.json",
						"inlineStyleLanguage": "less",
						"assets": [
							{
								"glob": "**/*",
								"input": "public"
							}
						],
						"styles": [
							"projects/angular-child/src/styles.less"
						],
						"scripts": [],
						"customWebpackConfig": {
							"path": "projects/angular-child/webpack.config.js"
						},
						"browser": "projects/angular-child/src/main.ts"
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "1MB",
									"maximumError": "2MB"
								}
							],
							"outputHashing": "all",
							"customWebpackConfig": {
								"path": "projects/angular-child/webpack.prod.config.js"
							}
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve-original": {
					"builder": "@angular-builders/custom-webpack:dev-server",
					"options": {
						"port": 4201
					},
					"configurations": {
						"production": {
							"buildTarget": "angular-child:esbuild:production"
						},
						"development": {
							"buildTarget": "angular-child:esbuild:development"
						}
					},
					"defaultConfiguration": "development"
				}
			}
		},
		"micro-frontends": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "less"
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": "dist/micro-frontends",
						"index": "src/index.html",
						"browser": "src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "tsconfig.app.json",
						"inlineStyleLanguage": "less",
						"assets": [
							{
								"glob": "**/*",
								"input": "public"
							}
						],
						"styles": [
							"src/styles.less"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "micro-frontends:build:production"
						},
						"development": {
							"buildTarget": "micro-frontends:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "tsconfig.spec.json",
						"inlineStyleLanguage": "less",
						"assets": [
							{
								"glob": "**/*",
								"input": "public"
							}
						],
						"styles": [
							"src/styles.less"
						],
						"scripts": []
					}
				}
			}
		}
	}
}